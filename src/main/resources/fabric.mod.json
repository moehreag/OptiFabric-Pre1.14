{
	"schemaVersion": 1,
	"id": "optifabric",
	"version": "${version}",
	"name": "Optifabric",
	"description": "Optifabric is a mod that loads optifine into fabric",
	"authors": [
		"Modmuss50",
		"Chocohead",
		"Hydos"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"mixins": [
		"optifabric.mixins.json"
	],
	"license": "CC0-1.0",
	"icon": "assets/optifabric/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"me.modmuss50.optifabric.mod.Optifabric"
		],
		"mm:early_risers": [
			"me.modmuss50.optifabric.mod.OptifabricSetup"
		]
	},
	"custom": {
		"fabric-renderer-api-v1:contains_renderer": true
	},
	"depends": {
		"fabricloader": ">=0.12.2",
		"minecraft": "1.8.9"
	},
	"accessWidener": "optifabric.accesswidener"
}
